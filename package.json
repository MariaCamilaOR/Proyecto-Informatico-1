{
  "name": "doyouremember",
  "version": "1.0.0",
  "description": "Sistema de Evaluación Cognitiva - DoYouRemember",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:gateway\" \"npm run dev:auth\" \"npm run dev:photo\" \"npm run dev:analysis\" \"npm run dev:report\" \"npm run dev:notification\" \"npm run dev:frontend\"",
    "dev:gateway": "cd services/api-gateway && npm run dev",
    "dev:auth": "cd services/auth-service && npm run dev",
    "dev:photo": "cd services/photo-service && npm run dev",
    "dev:analysis": "cd services/analysis-service && npm run dev",
    "dev:report": "cd services/report-service && npm run dev",
    "dev:notification": "cd services/notification-service && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:services && npm run build:frontend",
    "build:services": "npm run build:gateway && npm run build:auth && npm run build:photo && npm run build:analysis && npm run build:report && npm run build:notification",
    "build:gateway": "cd services/api-gateway && npm run build",
    "build:auth": "cd services/auth-service && npm run build",
    "build:photo": "cd services/photo-service && npm run build",
    "build:analysis": "cd services/analysis-service && npm run build",
    "build:report": "cd services/report-service && npm run build",
    "build:notification": "cd services/notification-service && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "test": "npm run test:services && npm run test:frontend",
    "test:services": "npm run test:gateway && npm run test:auth && npm run test:photo && npm run test:analysis && npm run test:report && npm run test:notification",
    "test:gateway": "cd services/api-gateway && npm test",
    "test:auth": "cd services/auth-service && npm test",
    "test:photo": "cd services/photo-service && npm test",
    "test:analysis": "cd services/analysis-service && npm test",
    "test:report": "cd services/report-service && npm test",
    "test:notification": "cd services/notification-service && npm test",
    "test:frontend": "cd frontend && npm test",
    "test:coverage": "npm run test:services:coverage && npm run test:frontend:coverage",
    "test:services:coverage": "npm run test:gateway:coverage && npm run test:auth:coverage && npm run test:photo:coverage && npm run test:analysis:coverage && npm run test:report:coverage && npm run test:notification:coverage",
    "test:gateway:coverage": "cd services/api-gateway && npm run test:coverage",
    "test:auth:coverage": "cd services/auth-service && npm run test:coverage",
    "test:photo:coverage": "cd services/photo-service && npm run test:coverage",
    "test:analysis:coverage": "cd services/analysis-service && npm run test:coverage",
    "test:report:coverage": "cd services/report-service && npm run test:coverage",
    "test:notification:coverage": "cd services/notification-service && npm run test:coverage",
    "test:frontend:coverage": "cd frontend && npm run test:coverage",
    "test:integration": "cd tests/integration && npm test",
    "migrate": "cd infrastructure/database && npm run migrate",
    "migrate:rollback": "cd infrastructure/database && npm run rollback",
    "seed": "cd infrastructure/database && npm run seed",
    "lint": "npm run lint:services && npm run lint:frontend",
    "lint:services": "npm run lint:gateway && npm run lint:auth && npm run lint:photo && npm run lint:analysis && npm run lint:report && npm run lint:notification",
    "lint:gateway": "cd services/api-gateway && npm run lint",
    "lint:auth": "cd services/auth-service && npm run lint",
    "lint:photo": "cd services/photo-service && npm run lint",
    "lint:analysis": "cd services/analysis-service && npm run lint",
    "lint:report": "cd services/report-service && npm run lint",
    "lint:notification": "cd services/notification-service && npm run lint",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:fix": "npm run lint:services:fix && npm run lint:frontend:fix",
    "lint:services:fix": "npm run lint:gateway:fix && npm run lint:auth:fix && npm run lint:photo:fix && npm run lint:analysis:fix && npm run lint:report:fix && npm run lint:notification:fix",
    "lint:gateway:fix": "cd services/api-gateway && npm run lint:fix",
    "lint:auth:fix": "cd services/auth-service && npm run lint:fix",
    "lint:photo:fix": "cd services/photo-service && npm run lint:fix",
    "lint:analysis:fix": "cd services/analysis-service && npm run lint:fix",
    "lint:report:fix": "cd services/report-service && npm run lint:fix",
    "lint:notification:fix": "cd services/notification-service && npm run lint:fix",
    "lint:frontend:fix": "cd frontend && npm run lint:fix",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d"
